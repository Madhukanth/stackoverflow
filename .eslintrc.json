{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "global-require": "off",
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
    "no-trailing-spaces": "off",
    "no-tabs": "off",
    "consistent-return": "off",
    "react/prefer-stateless-function": [0],
    "func-names": "off",
    "prefer-template": "off",
    "comma-dangle": "off",
    "comma-spacing": "off"
  }
}
